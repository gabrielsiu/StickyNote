		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>About</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>detail</key>
			<string>SNPAcknowledgementsController</string>
			<key>label</key>
			<string>Acknowledgements</string>
		</dict>
		<dict>
			<key>big</key>
			<true/>
			<key>cellClass</key>
			<string>HBTwitterCell</string>
			<key>height</key>
			<integer>56</integer>
			<key>label</key>
			<string>Gabriel Siu</string>
			<key>user</key>
			<string>gabrielsiu_dev</string>
		</dict>
	</array>
	<key>title</key>
	<string>StickyNote</string>
</dict>
</plist>
