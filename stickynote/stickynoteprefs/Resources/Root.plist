<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<!-- Enable/Disable Tweak -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Enable Tweak</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>isEnabled</string>
		</dict>
		<!-- Appearance -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Appearance</string>
			<key>footerText</key>
			<string>Default alpha value is 0.80.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Custom Note Color</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>useCustomNoteColor</string>
			<key>id</key>
			<string>useCustomNoteColorCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>cellClass</key>
			<string>PFSimpleLiteColorCell</string>
			<key>libcolorpicker</key>
			<dict>
				<key>defaults</key>
				<string>com.gabrielsiu.stickynoteprefs</string>
				<key>key</key>
				<string>noteColor</string>
				<key>fallback</key>
				<string>#FFFF00</string>
				<key>alpha</key>
				<false/>
			</dict>
			<key>label</key>
			<string>Note Color</string>
			<key>id</key>
			<string>noteColorCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Custom Font Color</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>useCustomFontColor</string>
			<key>id</key>
			<string>useCustomFontColorCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>cellClass</key>
			<string>PFSimpleLiteColorCell</string>
			<key>libcolorpicker</key>
			<dict>
				<key>defaults</key>
				<string>com.gabrielsiu.stickynoteprefs</string>
				<key>key</key>
				<string>fontColor</string>
				<key>fallback</key>
				<string>#000000</string>
				<key>alpha</key>
				<false/>
			</dict>
			<key>label</key>
			<string>Font Color</string>
			<key>id</key>
			<string>fontColorCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Custom Alpha</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>useCustomAlpha</string>
			<key>id</key>
			<string>useCustomAlphaCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSliderCell</string>
			<key>min</key>
			<real>0.0</real>
			<key>max</key>
			<real>1.0</real>
			<key>default</key>
			<real>0.8</real>
			<key>showValue</key>
			<true/>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>alphaValue</string>
			<key>id</key>
			<string>alphaCell</string>
		</dict>
		<!-- Sizing -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Sizing</string>
		</dict>
		<dict>
			<key>cellClass</key>
			<string>HBStepperTableCell</string>
			<key>default</key>
			<real>20</real>
			<key>label</key>
			<string>Font Size: %i</string>
