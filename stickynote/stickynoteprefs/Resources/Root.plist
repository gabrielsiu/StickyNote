			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>fontSize</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Corner Radius:</string>
			<key>isNumeric</key>
			<true/>
			<key>placeholder</key>
			<string>5</string>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>cornerRadius</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Width:</string>
			<key>isNumeric</key>
			<true/>
			<key>placeholder</key>
			<string>250</string>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>width</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Height:</string>
			<key>isNumeric</key>
			<true/>
			<key>placeholder</key>
			<string>250</string>
			<key>bestGuess</key>
			<string>250</string>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>height</string>
		</dict>
		<!-- Hide Button -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Hide Button</string>
			<key>footerText</key>
        	<string>Tip: Long press on the hide button to re-center the note.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>X-Axis Offset:</string>
			<key>isNumeric</key>
			<true/>
			<key>placeholder</key>
			<string>0</string>
			<key>bestGuess</key>
			<string>0</string>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>xOffset</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Y-Axis Offset:</string>
			<key>isNumeric</key>
			<true/>
			<key>placeholder</key>
			<string>0</string>
			<key>bestGuess</key>
			<string>0</string>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>yOffset</string>
		</dict>
		<!-- Animations -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Animations</string>
			<key>footerText</key>
			<string>Default duration for the hide animation is 0.20 seconds.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>detail</key>
			<string>SNPAnimationsListController</string>
			<key>label</key>
			<string>Hide Animation</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Custom animation duration</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>useCustomDuration</string>
			<key>id</key>
			<string>useCustomDurationCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSliderCell</string>
			<key>min</key>
			<real>0.0</real>
			<key>max</key>
			<real>5.0</real>
			<key>default</key>
			<real>0.2</real>
			<key>showValue</key>
			<true/>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>customDuration</string>
			<key>id</key>
			<string>durationCell</string>
		</dict>
